	"license": "MIT",
	"type": "module",
	"tldraw_template": {
		"repo": "tldraw/tldraw-sync-rivet",
		"cli": {
			"name": "Multiplayer",
			"description": "Real-time multiplayer with a self-hosted backend.",
			"shortDescription": "Real-time multiplayer.",
			"order": 2
		},
		"scripts": {
			"lint": null
		}
	},
	"scripts": {
		"dev": "concurrently \"vite\" \"tsx --watch server/server.ts\" --names \"vite,server\" --prefix-colors \"blue,green\"",
		"build": "tsc && vite build",
		"lint": "yarn run -T tsx ../../internal/scripts/lint.ts",
		"preview": "vite preview"
	},
	"dependencies": {
		"@hono/node-server": "^1.19.5",
		"@hono/node-ws": "^1.2.0",
