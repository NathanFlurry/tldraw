		"@tldraw/sync": "workspace:*",
		"@tldraw/sync-core": "workspace:*",
		"@tldraw/tlschema": "workspace:*",
		"itty-router": "^5.0.18",
		"lodash.throttle": "^4.1.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.28.2",
		"rivetkit": "https://pkg.pr.new/rivet-dev/rivet/rivetkit@ea8ef90",
		"tldraw": "workspace:*"
	},
	"devDependencies": {
